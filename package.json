{
  "name": "ts-react-tmpl",
  "version": "0.0.0",
  "description": "",
  "main": "src/index.tsx",
  "scripts": {
    "prebuild": "npm run clean:dist",
    "build": "bash scripts/build.sh",
    "build:analyze": "cross-env-shell BUILD_ANALYZE=true bash ./scripts/build.sh",
    "build:loc": "bash scripts/build.loc.sh",
    "build:loc:analyze": "cross-env-shell BUILD_ANALYZE=true bash ./scripts/build.loc.sh",
    "cm": "npx git-cz",
    "check:all": "run-p lint:fix types:check",
    "clean:dist": "rm -rf dist",
    "deploy:cdn": "bash scripts/deploy-to-cdn.sh",
    "devops:chmod-scripts": "bash devops/local/scripts/chmod-scripts.sh",
    "launch:loc": "run-p launch:dev-server types:watch",
    "launch:dev-server": "bash ./scripts/launch.dev-server.sh",
    "lint": "npx eslint src --config ./configs/eslint.config.js  --ext .ts,.tsx,.json --ignore-pattern '/node_modules/*' --ignore-pattern '**/*.d.ts' --ignore-pattern '**/*.spec.ts' --ignore-pattern '/src/assets/**'",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run launch:loc",
    "types:check": "tsc -p tsconfig.json",
    "types:watch": "npm run types:check -- --watch",
    "watch": "run-p types:watch watch:loc",
    "watch:loc": "bash scripts/watch.loc.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check:all",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --config=./configs/commitlint.config.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "author": "Oleksii Honchar",
  "license": "MIT",
  "dependencies": {
    "env-cmd": "10.1.0",
    "npm-run-all": "4.1.5"
  },
  "devDependencies": {
    "@ciklum/logan": "0.4.10",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@hapi/joi": "17.1.0",
    "@types/hapi__joi": "16.0.12",
    "@types/lodash": "4.14.149",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/uuid": "7.0.0",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "classnames": "2.2.6",
    "color": "3.1.2",
    "commitizen": "4.0.3",
    "compression-webpack-plugin": "3.1.0",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "cssnano": "4.1.10",
    "cssnano-preset-default": "4.0.7",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-standard": "14.1.0",
    "eslint-config-standard-preact": "1.1.6",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-json": "2.1.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "eslint-plugin-standard": "4.0.1",
    "extract-css-chunks-webpack-plugin": "^4.7.4",
    "file-loader": "5.1.0",
    "handlebars": "4.7.3",
    "husky": "4.2.3",
    "lodash": "4.17.15",
    "lodash-webpack-plugin": "0.11.5",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "rimraf": "3.0.2",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.3",
    "sugarss": "2.0.0",
    "svg-inline-loader": "0.8.2",
    "tailwindcss": "1.2.0",
    "to-string-loader": "1.1.6",
    "ts-loader": "6.2.1",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.8.3",
    "typescript-plugin-css-modules": "2.2.0",
    "typings-for-css-modules-loader": "1.7.0",
    "webpack": "4.42.0",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "engines": {
    "node": "12"
  }
}
