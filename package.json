{
  "name": "ts-react-tmpl",
  "version": "1.0.0",
  "description": "",
  "author": "Oleksii Honchar",
  "main": "src/index.tsx",
  "type": "module",
  "scripts": {
    "build": "make build",
    "build:analyze": "make build-analyze",
    "build:loc": "make build-loc",
    "build:loc:analyze": "make build-loc-analyze",
    "cm": "npx git-cz",
    "check:all": "run-p lint:fix types:check",
    "deploy:cdn": "bash scripts/deploy-to-cdn.sh",
    "devops:chmod-scripts": "bash devops/local/scripts/chmod-scripts.sh",
    "launch:loc": "run-p launch:dev-server types:watch",
    "launch:dev-server": "make launch-dev-server",
    "lint": "npx eslint src --config eslintrc.js  --ext .ts,.tsx,.json --ignore-pattern '/node_modules/*' --ignore-pattern '**/*.d.ts' --ignore-pattern '**/*.spec.ts' --ignore-pattern '/src/assets/**'",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run launch:loc",
    "types:check": "tsc -p configs/tsconfig.all-in-one.json",
    "types:watch": "npm run types:check -- --watch",
    "watch": "run-p types:watch watch:loc",
    "watch:loc": "make watch-loc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check:all",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --config=./configs/commitlint.config.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@tsconfig/node20": "^1.0.1",
    "@tsconfig/recommended": "^1.0.2",
    "@types/colors": "^1.2.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.2.5",
    "colors": "^1.4.0",
    "env-cmd": "10.1.0",
    "minimist": "^1.2.8",
    "node-emoji": "^2.0.2",
    "npm-run-all": "4.1.5",
    "ora": "^6.3.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@ciklum/logan": "0.4.10",
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@hapi/joi": "17.1.0",
    "@types/color": "^3.0.3",
    "@types/hapi__joi": "17.1.9",
