    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.8.1",
    "cssnano": "6.0.1",
    "cssnano-preset-default": "6.0.1",
    "cz-conventional-changelog": "3.3.0",
    "esbuild-loader": "^3.0.1",
